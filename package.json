    "@umijs/fabric": "^2.14.1",
    "@umijs/test": "^3.5.41",
    "dumi": "^1.1.54",
    "father-build": "^1.22.5",
    "gh-pages": "^3.2.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.8.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.76.0",
    "typescript": "^4.9.5",
    "yorkie": "^2.0.0"
