    "type": "https"
  },
  "keywords": [
    "mui",
    "material-ui",
    "antd",
    "image crop",
    "image-crop",
    "iimm",
    "www.iimm.ink",
    "liudichen",
    "Upload",
    "react",
    "@mui/material",
    "image cut",
    "preview",
    "dropzone",
    "react"
  ],
  "license": "MIT",
  "files": [
    "es",
    "lib"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
